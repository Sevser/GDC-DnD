    "SchoolOfMagic": {
      "type": "component",
      "repeatable": false,
      "component": "enums.school-of-magic"
    },
    "class": {
      "type": "component",
      "repeatable": true,
      "component": "enums.class"
    },
    "archetypes": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::archetype.archetype",
      "mappedBy": "spells"
    },
    "activeTime": {
      "type": "integer",
      "min": 0,
