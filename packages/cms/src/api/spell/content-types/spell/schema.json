    "cast_times": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::cast-time.cast-time",
      "inversedBy": "spells"
    },
    "attacks": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::attack.attack",
      "inversedBy": "spells"
    },
    "ActionType": {
      "type": "component",
      "repeatable": false,
      "component": "enums.action-type"
    }
